 	.syntax unified
 	.cpu cortex-m0
 	.align	2
 	.global	GetFib
 	.thumb
 	.thumb_func
 GetFib:
		push	{r7, lr}
		sub	sp, sp, #4
  		add	r7, sp, #0
  		str	r0, [r7]
  		ldr	r1, [r7]
  		// convert to nxp commands?
  		movs r3,#3
  		movs r11,#1
  		movs r10,#1
  		LOOP:
  		adds r2,r5,r11
  		mov r11,r10
  		mov r10,r2
  		adds r3,r3,#1
  		cmp     r3, r1
        blt     LOOP

  		mov r0,r2
  		mov	sp, r7
  		add	sp, sp, #4
		pop	{r7, pc}
