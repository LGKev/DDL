 	.syntax unified
 	.cpu cortex-m0
 	.align	2
 	.global	GetFib
 	.thumb
 	.thumb_func
 GetFib:
		push	{r7, lr}
		sub	sp, sp, #4
  		add	r7, sp, #0
  		str	r0, [r7]
  		ldr	r1, [r7]
  		// convert to nxp commands?
  		movs r3,#3
  		movs r7,#1
  		movs r5,#1
  		LOOP:
  		adds r1,r5,r7
  		mov r7,r5
  		mov r5,r1
  		adds r3,r3,#1
  		cmp     r3, r1
        blt     LOOP

  		mov r0,r1
  		mov	sp, r7
  		add	sp, sp, #4
		pop	{r7, pc}
